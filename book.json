{
    "title": "牛马程序员",
    "author": "爆龙战士",
    "description": "三更灯火五更鸡，正是男儿读书时",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "links": {
        "sidebar": {
            "简陋的主页": "http://gatesma.cn"
        }
    },
    "plugins": [
        "favicon-absolute",
        "toc",
        "github-buttons",
        "-sharing",
        "sharing-plus",
        "code",
        "advanced-emoji",
        "copyright",
        "donate",
        "expandable-chapters-small",
        "page-toc",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "favicon-absolute": {
            "favicon": "./img/favicon.ico",
            "appleTouchIconPrecomposed152": "http://cos.gatesma.cn/engineer.png"
        },
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "github-buttons": {
            "buttons": [{
                "user": "gatesma",
                "repo": "my_realm",
                "type": "star",
                "size": "small"
            }]
        },
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper", "linkedin",
                "pocket", "stumbleupon"
            ]
        },
        "copyright": {
            "site": "https://gatesma.github.io/my_realm/",
            "author": "爆龙战士",
            "website": "牛马程序员",
            "image": "",
            "copyProtect": false
        },
        "donate": {
            "wechat": "http://cos.gatesma.cn/wechat-donate.png",
            "alipay": "http://cos.gatesma.cn/alipay-donate.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝",
            "wechatText": "微信"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "tbfed-pagefooter": {
            "copyright": "",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}